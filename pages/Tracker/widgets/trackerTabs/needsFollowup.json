          "onClick": "{{(async () => {\n    const selectedRow = needsFollowup.triggeredRow; \n    const newOnboardsId = selectedRow ? selectedRow.id : null; \n    if (!newOnboardsId) {\n      showAlert(\"New Onboard ID not found. Please try again.\", \"error\");\n      return;\n    } \n\t NeedFollowupTextBox.setValue(newOnboardsId); newOnboardsTextBoxInfoPending.setDisabled(true);   \n\t\n\tshowModal(needFollowupModal.name)\n\t\n  })()\n}}",
